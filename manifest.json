
{
  "manifest_version": 3,
  "name": "Group Notify Extension",
  "version": "1.0",
  "permissions": ["webRequest", "storage", "tabs", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  }
}
